{
  "name": "geardev/core",
  "description": "Core. Part of toolkit to boost your development with Laravel and geardev.io",
  "autoload": {
    "psr-4": {
      "GearDev\\Core\\": "src/"
    }
  },
  "bin": [
    "bin/gear.php",
    "bin/gear-dev-server.php",
    "bin/gear-server.php"
  ],
  "require": {
    "php": "^8.2||^8.3",
    "nette/php-generator": "v4.1.5",
    "laravel/framework": "^11.0",
    "geardev/collector": "^0.0",
    "geardev/coroutines": "^0.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "GearDev\\Core\\GearCoreLaravelProvider"
      ]
    }
  }
}